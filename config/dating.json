{
  "services": {
    "declarationsPath": "../ota-services-dating/declarations",
    "documentTypesPath": "../ota-services-dating/document-types.json"
  },
  "recorder": {
    "versions": {
      "storage": {
        "git": {
          "path": "../ota-versions-dating",
          "publish": true,
          "prefixMessageToSnapshotId": "This version was recorded after filtering snapshot https://github.com/OpenTermsArchive/snapshots-dating/commit/",
          "author": {
            "name": "Open Terms Archive Bot",
            "email": "bot@opentermsarchive.org"
          }
        }
      }
    },
    "snapshots": {
      "storage": {
        "git": {
          "path": "../ota-snapshots-dating",
          "publish": true,
          "author": {
            "name": "Open Terms Archive Bot",
            "email": "bot@opentermsarchive.org"
          }
        }
      }
    }
  },
  "logger": {
    "sendMailOnError": {
      "to": "admin@opentermsarchive.org",
      "from": "noreply@opentermsarchive.org",
      "sendWarnings": false
    }
  },
  "notifier": {
    "sendInBlue": {
      "administratorsListId": 9,
      "updatesListId": 191
    }
  }
}

