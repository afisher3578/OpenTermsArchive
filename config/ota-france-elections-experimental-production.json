{
  "services": {
    "declarationsPath": "../declarations/declarations"
  },
  "recorder": {
    "versions": {
      "storage": {
        "git": {
          "path": "../versions",
          "prefixMessageToSnapshotId": "This version was recorded after filtering snapshot https://github.com/OpenTermsArchive/france-elections-experiment-snapshots/commit/",
          "publish": true
        }
      }
    },
    "snapshots": {
      "storage": {
        "git": {
          "path": "../snapshots",
          "publish": true
        }
      }
    }
  },
  "fetcher": {
    "language": "fr"
  },
  "logger": {
    "sendMailOnError": {
      "to": "admin@opentermsarchive.org",
      "from": "noreply@opentermsarchive.org",
      "sendWarnings": false
    }
  },
  "tracker": {
    "githubIssues": {
      "repository": "OpenTermsArchive/declarations-france-elections-experiment"
    }
  },
  "dataset": {
    "servicesRepositoryName": "declarations-france-elections",
    "versionsRepositoryURL": "https://github.com/OpenTermsArchive/versions-france-elections-experiment"
  }
}
